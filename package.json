{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest --coverage --no-cache"
  },
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "transformIgnorePatterns": ["node_modules/(?!((jest-)?react-native|react-router-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation))"],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "expo": "29.0.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-native-elements": "0.19.1"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.1",
    "@types/enzyme-adapter-react-16": "1.0.0",
    "@types/expo": "27.0.6",
    "@types/jest": "^23.3.1",
    "@types/react": "16.4.7",
    "@types/react-native": "0.56.4",
    "@types/react-test-renderer": "16.0.1",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "jest": "23.4.2",
    "jest-cli": "23.4.2",
    "jest-expo": "29.0.0",
    "react-native-typescript-transformer": "1.2.10",
    "ts-jest": "23.0.1",
    "typescript": "2.9.2"
  }
}
